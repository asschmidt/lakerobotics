{
    "configurations": [
        {
            "name": "LRPlatform ARM",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:DEVENV_ROOT_PATH}/gcc-arm-none-eabi-10-2020-q4-major/arm-none-eabi/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "${env:DEVENV_ROOT_PATH}/gcc-arm-none-eabi-10-2020-q4-major/bin/arm-none-eabi-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm"
