        {
            "name": "LRPlatform AVR",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:DEVENV_ROOT_PATH}/avr-gcc-10.1.0-x64-windows/avr/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "${env:DEVENV_ROOT_PATH}/avr-gcc-10.1.0-x64-windows/bin/avr-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
